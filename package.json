{
  "name": "react-clocks",
  "version": "1.0.0",
  "description": "Refactoring my original clocks Repo with React components",
  "main": "index.js",
  "scripts": {
    "bundle": "browserify public0/main.js -t babelify -o public0/bundle.js",
    "bundle:watch":
      "watchify public0/main.js -vd -t babelify -o public0/bundle.js",
    "lint": "run-p lint:*",
    "lint:html": "htmlhint ./**/*.html",
    "lint:css": "stylelint ./**/*.css",
    "lint:js": "eslint ./**/*.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "precommit": "npm test",
    "start": "node index.js",
    "seed": "node seed.js",
    "watch": "nodemon index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rafbatista/react-clocks.git"
  },
  "keywords": [],
