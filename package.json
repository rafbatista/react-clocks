  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rafbatista/react-clocks/issues"
  },
  "homepage": "https://github.com/rafbatista/react-clocks#readme",
  "dependencies": {
    "babel-core": "6.26.0",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.0",
    "babelify": "8.0.0",
    "browserify": "14.5.0",
    "express": "^4.16.2",
    "moment": "^2.19.1",
    "moment-timezone": "^0.5.13",
    "mongodb": "^2.2.33",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "watchify": "3.9.0"
  },
  "devDependencies": {
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "eslint": "4.5.0",
