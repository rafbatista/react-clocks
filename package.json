    "eslint-plugin-node": "5.1.1",
    "eslint-plugin-promise": "3.5.0",
    "eslint-plugin-react": "7.3.0",
    "eslint-plugin-standard": "3.0.1",
    "htmlhint": "0.9.13",
    "husky": "0.14.3",
    "nodemon": "^1.12.1",
    "npm-run-all": "4.1.1",
    "stylelint": "8.0.0",
    "stylelint-config-standard": "17.0.0",
    "watchify": "^3.9.0"
  }
